{
  "manifest_version": 2,

  "name": "Find your perfect color",
  "description": "This extension will find you the perfect color!! ",
  "version": "1.0",
  "icons": {
    "16" : "./16.png",
    "32" : "./32.png",
    "48" : "./48.png",
    "128": "./128.png"
  },
  "background" : {
    "scripts" : ["./background.js"]
  }, 
  "options_page" : "./optional.html",
  "browser_action" : {
    "default_popup" : "popup.html",
    "default_title": "hello world"
  },
  "permissions": [
    "tabs",
    "https://www.google.com/*"
   ] 
}